<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <link rel="icon" type="image/x-icon" href="/assets/favicon.ico">
    <link rel="stylesheet" href="/css/global-page.css">

    <title>Global stats</title>
</head>
<body>
    <%- include('../partials/navbar.ejs')%>

    <main>

        <div id="top-tracks">
            top tracks
            <ul>
                <% topTracks.forEach((track) => {%>
                    <li> <%= track.trackName %> - <%= track.playCount %></li>
                <% })%>
            </ul>
        </div>
        <div id="top-artists">
            top artists
            <ul>
                <% topArtists.forEach((track) => {%>
                    <li> <%= track.artistName %> - <%= track.playCount %></li>
                <% })%>
            </ul>
        </div>

    </main>

</body>
</html>